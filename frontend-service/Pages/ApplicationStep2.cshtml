@page
@model frontend_service.ApplicationStep2Model
@using Models
@{
    ViewData["Title"] = "Выбор места слияния диаграмм";
    string token = Request.Cookies["token_"];
    var index = Model.filejobPage2.IndexClientData;
    Layout = "";
    /* //test info//
    <p>UC: @Model.UserChoice</p>
    <p>YourData: @index</p>
     */
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="Сервис интеграции диаграмм" />
    <meta name="author" content="iddqd" />
    <title>IS2-Union - Приложение</title>
    <link rel="icon" type="image/x-icon" href="~/Files/logo.png" />
    <script src="https://use.fontawesome.com/releases/v5.13.0/js/all.js" crossorigin="anonymous"></script>
    <link href="https://fonts.googleapis.com/css?family=Varela+Round" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet" />
    <link href="css/styles.css" rel="stylesheet" />
</head>
<body id="page-top">
    <!--nav-->
    <nav class="navbar navbar-expand-lg navbar-light fixed-top" id="mainNav">
        <div class="container">
            <a class="navbar-brand js-scroll-trigger" href="#page-top">IS2-Union</a><button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">Menu<i class="fas fa-bars"></i></button>
            <div class="collapse navbar-collapse" id="navbarResponsive">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="~/Index">Главная</a></li>
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="~/Index#about">О приложении</a></li>
                    <li class="nav-item"><a class="nav-link js-scroll-trigger" href="#contacts">Контакты</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <!-- Signup-->
    <section class="signup-section" id="signup">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 mx-auto text-center">
                    <h2 class="text-white mb-4">
                        Продолжим?
                    </h2>
                    <p class="text-white-50">
                        Теперь необходимо выбрать место слияния диаграмм.
                    </p>
                </div>
            </div>
        </div>
    </section>
    <!--integrationblock-->
    <section class="projects-section bg-light" id="application">
        <div class="container">
            <hr />
            <form class="form-group" method="post">
                <h2>Шаг 4</h2>
                <p>Выберите элемент для интеграции</p>
                <div class="input-group">
                    <select class="custom-select" id="dropdown_select" asp-for="SelectId" name="elementsId" asp-items="Startup.clientData[index].Options"></select>
                    <div class="input-group-append">
                        <input type="submit" value="Интегрировать" class="btn btn-primary" />
                    </div>
                </div>
            </form>
            <hr />
        </div>
    </section>
    <!-- Projects-->
    <section class="projects-section bg-light" id="projects2">
        <div class="container">
           
        </div>
    </section>
    <!--footer-->
    <footer class="footer bg-black small text-center text-white-50">
        <div class="container">
        </div>
    </footer>
    <!--footer-->
    <footer class="footer bg-black small text-center text-white-50">
        <div class="container">
            &copy;@DateTime.Now.Year -
            <a asp-area="" asp-page="/Index">
                IS2service @Startup.versionShow
            </a>
        </div>
    </footer>
    <!-- Bootstrap core JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.bundle.min.js"></script>
    <!-- Third party plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    <!-- Core theme JS-->
    <script src="js/scripts.js"></script>
</body>
